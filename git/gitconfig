  ui = auto
  interactive = auto
  diff = auto
[core]
  editor = vim
  filemode = false
  autocrlf = input
  excludesfile = ~/.gitignore_global
  whitespace = cr-at-eol
[i18n]
  logOutputEncoding = utf-8
[gui]
  encoding = utf-8
[alias]
  co = 'checkout'
  st = 'status'
  di = 'diff'
  cm = 'commit'
  br = 'branch'
  fe = 'fetch'
  rb = 'rebase'
  pl = 'pull'
  ps = 'push'
  tr = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
